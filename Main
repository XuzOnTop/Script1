
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Personal Script", "DarkTheme")


--Main
local Main = Window:NewTab("Main Script")
local MainSection = Main:NewSection("Scripts")


MainSection:NewButton("Infinite Yield", "Executes the script shown", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)


MainSection:NewButton("Tp Spam", "Executes the script shown", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/XuzOnTop/Script1/main/Tp%20Spam'))()
end)


MainSection:NewButton("Rejoin", "Executes the script shown", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/XuzOnTop/Script1/main/Rejoin'))()
end)


--Main
local Main = Window:NewTab("Rizz")
local MainSection = Main:NewSection("Rizz")

MainSection:NewButton("School Bus Rizz", "Executes the script shown", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/XuzOnTop/Script1/main/School%20Bus%20Rizz'))()
end)

